<view style="width: 100vw; height: 100vh; background-color: #fff; position: absolute; left:0; top:0; z-index:9998; overflow: hidden;">
  <view wx:for="{{skeletonRectLists}}" wx:key="index" style="background-color: #f2f2f2; position: absolute; width: {{item.width}}px; height: {{item.height}}px; left: {{item.left}}px; top: {{item.top}}px"></view>
  <view wx:for="{{skeletonCircleLists}}" wx:key="index" style="background-color: #f2f2f2; position: absolute; width: {{item.width}}px; height: {{item.height}}px; border-radius: {{item.width}}px; left: {{item.left}}px; top: {{item.top}}px"></view>
</view>